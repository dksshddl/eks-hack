  karpenter                    = "1.9.0"
  istio                        = "1.20.0"
  argo                         = "latest"
  prometheus                   = ""
  grafana                      = "7.0.0"
  keda                         = "2.12.0"
  cluster-autoscaler           = "9.37.0"
  aws-load-balancer-controller = "1.7.1"
}
